const float scroll_speed = 1.;

void fragment() {
	vec2 shifteduv = UV;
	float x_offset = -.05;
    shifteduv.x += (cos(TIME * 3.) / 20. * scroll_speed) + x_offset;
    vec4 color = texture(TEXTURE, shifteduv);
    COLOR = color;
}"

