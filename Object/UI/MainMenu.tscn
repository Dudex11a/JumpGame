code = "shader_type canvas_item;

//void fragment() {
//	COLOR = texture(TEXTURE, UV);
//	COLOR.b = cos(TIME * 10.0);
//}

const float scroll_speed = 1.;

void fragment() {
	vec2 shifteduv = UV;
	float x_offset = -.05;
    shifteduv.x += (cos(TIME * 3.) / 20. * scroll_speed) + x_offset;
    vec4 color = texture(TEXTURE, shifteduv);
    COLOR = color;
}"

